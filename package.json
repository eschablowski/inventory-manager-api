{
  "name": "@eschablowski/inventory-manager-api",
  "version": "0.1.5",
  "description": "Types for the Inventory Management Software",
  "main": "build/index.js",
  "types": "src/index.ts",
  "repository": "https://github.com/eschablowski/inventory-manager-api",
  "author": "Elias Schablowski <eschablowski@me.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "run-s build:prod",
    "build:dev": "webpack --config ./config/webpack.config.dev.ts",
    "build:prod": "webpack --config ./config/webpack.config.prod.ts",
    "watch": "run-s watch:dev",
    "watch:dev": "webpack --config ./config/webpack.config.dev.ts --watch",
    "watch:prod": "webpack --config ./config/webpack.config.prod.ts --watch",
    "release": "release-it",
    "commit": "run-s git-add git-cz",
    "git-add": "git-add",
    "git-cz": "git-cz",
    "apollo": "run-p apollo:*",
    "apollo:server": "nodemon apollo/index.ts",
    "apollo:watch": "yarn watch:dev",
    "swagger": "run-p swagger:*",
    "swagger:server": "nodemon swagger/server.js",
    "swagger:watch": "yarn watch:dev"
  },
  "nodemonConfig": {
    "watch": [
      "build",
      "swagger",
      "apollo"
    ],
    "ext": "*.*"
  },
  "devDependencies": {
    "@graphql-toolkit/common": "^0.10.4",
    "@graphql-toolkit/core": "^0.10.4",
    "@graphql-toolkit/schema-merging": "^0.10.4",
    "@types/crc": "^3.4.0",
    "@types/express-openapi": "^1.9.0",
    "@types/faker": "^4.1.11",
    "@types/file-loader": "4.2.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "8.0.3",
    "@types/js-yaml": "^3.12.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/webpack": "^4.41.11",
    "@types/webpack-merge": "4.1.5",
    "@types/webpack-node-externals": "^1.7.1",
    "@types/xml": "^1.0.4",
    "@types/yaml": "1.2.0",
    "apollo-server": "^2.12.0",
    "express": "^4.17.1",
    "express-openapi": "^6.0.0",
    "faker": "^4.1.0",
    "file-loader": "^6.0.0",
    "git-add": "^0.0.2",
    "git-cz": "^4.3.1",
    "js-yaml": "^3.13.1",
    "json-schema-ref-parser": "^8.0.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.3",
    "openapi-types": "^1.3.5",
    "pnp-webpack-plugin": "^1.6.4",
    "raw-loader": "^4.0.0",
    "release-it": "^13.5.2",
    "swagger-ui-express": "^4.1.4",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "yaml-import-loader": "^1.3.6",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "crc": "^3.8.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "xml": "^1.0.1"
  },
  "resolutions": {
    "source-map": "0.7.3"
  },
  "dependenciesMeta": {
    "source-map": {
      "unplugged": true
    }
  }
}
