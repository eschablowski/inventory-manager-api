{
  "name": "@eschablowski/inventory-manager-api",
  "version": "0.1.5",
  "description": "Types for the Inventory Management Software",
  "main": "build/index.js",
  "types": "src/index.ts",
  "repository": "https://github.com/eschablowski/inventory-manager-api",
  "author": "Elias Schablowski <eschablowski@me.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "run-s build:prod",
    "build:dev": "webpack --config ./config/webpack.config.dev.ts",
    "build:prod": "webpack --config ./config/webpack.config.prod.ts",
    "watch": "run-s watch:dev",
    "watch:dev": "webpack --config ./config/webpack.config.dev.ts --watch",
    "watch:prod": "webpack --config ./config/webpack.config.prod.ts --watch",
    "release": "release-it",
    "commit": "git-cz",
    "git-cz": "git-cz",
    "apollo": "run-p apollo:*",
    "apollo:server": "nodemon apollo/index.ts",
    "apollo:watch": "yarn watch:dev",
    "swagger": "run-p swagger:*",
    "swagger:server": "nodemon swagger/server.js",
    "swagger:watch": "yarn watch:dev"
  },
  "nodemonConfig": {
    "watch": [
      "build",
      "swagger",
      "apollo"
    ],
    "ext": "*.*"
  },
  "devDependencies": {
    "@graphql-tools/core": "^5.0.1-alpha-78275ec.0",
    "@graphql-tools/merge": "^6.0.5",
    "@graphql-tools/utils": "^6.0.5",
    "@openapi-contrib/openapi-schema-to-json-schema": "^3.0.0",
    "@types/crc": "^3.4.0",
    "@types/faker": "^4.1.12",
    "@types/file-loader": "4.2.0",
    "@types/graphql-upload": "8.0.3",
    "@types/js-yaml": "^3.12.4",
    "@types/lodash": "^4.14.154",
    "@types/node": "^14.0.9",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/webpack": "^4.41.17",
    "@types/webpack-merge": "4.1.5",
    "@types/webpack-node-externals": "^1.7.1",
    "@types/xml": "^1.0.5",
    "apollo-server": "^2.14.1",
    "express": "^4.17.1",
    "express-openapi": "^6.0.0",
    "faker": "^4.1.0",
    "file-loader": "^6.0.0",
    "git-cz": "^4.6.2",
    "graphql-tag": "^2.10.3",
    "js-yaml": "^3.14.0",
    "json-loader": "^0.5.7",
    "json-schema-ref-parser": "^9.0.1",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.4",
    "openapi-types": "^1.3.5",
    "pnp-webpack-plugin": "^1.6.4",
    "raw-loader": "^4.0.1",
    "release-it": "^13.6.2",
    "snyk": "^1.334.0",
    "swagger-ui-express": "^4.1.4",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.7",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "yaml-import-loader": "^1.3.6",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "ajv": "^6.12.2",
    "crc": "^3.8.0",
    "graphql": "^15.0.0",
    "xml": "^1.0.1"
  },
  "dependenciesMeta": {
    "source-map": {
      "unplugged": true
    }
  }
}
